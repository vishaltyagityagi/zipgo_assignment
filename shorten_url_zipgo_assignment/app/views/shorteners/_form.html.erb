<div class="form-group">
  <%= form_with(model: shortener, local: true) do |form| %>
    <% if shortener.errors.any? %>
      <div id="error_explanation">
        <h2>
          <%= pluralize(shortener.errors.count, "error") %>
          prohibited this shortener from being saved:
        </h2>
        <ul>
          <% shortener.errors.full_messages.each do |message| %>
            <li>
              <%= message %>
            </li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <label for="email">
      <%= form.label :url %>
    </label>
    <%= form.text_field :url, id: :shortener_url, class: "form-control", id: "email" %>
    <%= form.submit %>
  <% end %>
  <br/>
  <br/>
  <br/>
  <br/>
</div>
<%= form_tag import_shorteners_path, multipart: true do %>
  <div class="field">
    <%= label_tag :Upload_csv_file %>
    <%= file_field_tag :file %>
  </div>
  <div class="actions">
    <%= submit_tag 'Submit' %>
  </div>
<% end %>
